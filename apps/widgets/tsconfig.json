	"include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "src/utils/module.d.ts"],
	"exclude": ["node_modules"],
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"pages/*": ["./src/pages/*"],
