  "private": true,
  "scripts": {
    "dev": "metacraft",
    "dev:staging": "metacraft --envFile .env.staging --port 3001",
    "dev:prod": "metacraft --envFile .env.production --port 3002",
    "dev:ext": "BUILD_TARGET=extension && metacraft",
    "android": "react-native run-android",
    "build:all": "yarn build:ext && yarn build:web",
    "build:web": "rimraf metacraft builds && metacraft bundle",
    "build:web:dev": "rimraf metacraft builds && metacraft --envFile .env.development bundle",
    "build:web:staging": "rimraf metacraft builds && metacraft --envFile .env.staging bundle",
    "build:ext": "BUILD_TARGET=extension && yarn build:web && node browser/bundler",
    "build:ext:dev": "BUILD_TARGET=extension && yarn build:web:staging && node browser/bundler",
    "build:ext:staging": "BUILD_TARGET=extension && yarn build:web:staging && node browser/bundler",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "start": "react-native start",
    "postinstall": "rm -rf node_modules/superstruct",
    "test": "jest --passWithNoTests"
  },
