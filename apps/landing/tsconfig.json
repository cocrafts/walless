{
  "extends": "@walless/tsconfig/next.json",
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "components/*": [
        "./components/*"
      ],
      "pages/*": [
        "./pages/*"
      ],
      "utils/*": [
        "./utils/*"
      ],
      "state/*": [
        "./src/state/*"
      ],
    }
  }
}
