{
  "name": "walless",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "batch": "node tool/batch",
    "build": "node tool/batch && turbo run build",
    "deploy": "sst deploy",
    "deploy:wallet": "sst deploy wallet",
    "deploy:document": "sst deploy document",
    "deploy:landing": "sst deploy landing",
    "dev": "turbo run dev",
    "dev:sst": "sst dev --stage development",
    "test": "turbo run test",
    "gen": "node tool/gen",
    "codegen": "graphql-codegen --config tool/codegen.ts",
    "invalidate": "node tool/invalidate",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "type-check": "turbo run type-check",
    "postinstall": "yarn batch",
    "reset": "find . -type dir -name node_modules | xargs rm -rf && rm -rf yarn.lock"
  },
  "dependencies": {
    "@tkey/chrome-storage": "9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/client-preset": "4.1.0",
    "@graphql-codegen/typescript": "^4.0.0",
    "@graphql-codegen/typescript-resolvers": "^4.0.0",
    "@jest/globals": "^29.4.3",
    "@next/eslint-plugin-next": "^13.5.4",
    "@react-native-clipboard/clipboard": "^1.13.0",
    "@solana/spl-account-compression": "^0.2.0",
    "@types/chrome": "^0.0.231",
    "@types/lodash": "^4.14.191",
    "@types/node": "^17.0.12",
    "@types/numeral": "^2.0.2",
    "@types/pouchdb-adapter-http": "^6.1.3",
    "@types/pouchdb-adapter-idb": "^6.1.4",
    "@types/pouchdb-adapter-websql": "^6.1.4",
    "@types/pouchdb-core": "^7.0.11",
    "@types/pouchdb-mapreduce": "^6.1.7",
    "@types/pouchdb-replication": "^6.4.4",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.7",
    "@types/react-native": "^0.71.2",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@walless/eslint-config": "*",
    "@walless/tsconfig": "*",
    "@wasm-tool/wasm-pack-plugin": "^1.6.0",
    "archiver": "^5.3.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "constructs": "10.2.69",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "eslint": "^8.51.0",
    "fs-extra": "^11.1.0",
    "jest": "^29.4.3",
    "metro-react-native-babel-preset": "0.73.9",
    "mustache": "^4.2.0",
    "prettier": "^3.0.3",
    "react-native": "^0.71.14",
    "react-native-buffer": "^6.0.3",
    "react-native-gesture-handler": "^2.13.4",
    "react-native-reanimated": "^3.6.1",
    "rimraf": "^5.0.0",
    "shx": "^0.3.4",
    "sst": "2.41.4",
    "superstruct": "^1.0.3",
    "ts-jest": "^29.0.5",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "turbo": "latest",
    "typescript": "^5.4.0",
    "webpack": "^5.89.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@4.1.1",
  "resolutions": {
    "@react-native-firebase/app@18.6.1": "patch:@react-native-firebase/app@npm%3A18.6.1#./.yarn/patches/@react-native-firebase-app-npm-18.6.1-ef99d77745.patch"
  }
}
