{
  "name": "walless",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "batch": "node tool/batch",
    "build": "node tool/batch && turbo run build",
    "deploy": "sst deploy",
    "dev": "turbo run dev",
    "test": "turbo run test",
    "gen": "node tool/gen",
    "codegen": "graphql-codegen --config tool/codegen.ts",
    "invalidate": "node tool/invalidate",
    "lint": "turbo run lint",
    "postinstall": "yarn batch",
    "reset": "find . -type dir -name node_modules | xargs rm -rf && rm -rf yarn.lock"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/client-preset": "4.1.0",
    "@graphql-codegen/typescript": "^4.0.0",
    "@graphql-codegen/typescript-resolvers": "^4.0.0",
    "@jest/globals": "^29.4.3",
    "@types/chrome": "^0.0.231",
    "@types/lodash": "^4.14.191",
    "@types/node": "^17.0.12",
    "@types/numeral": "^2.0.2",
    "@types/pouchdb-adapter-http": "^6.1.3",
    "@types/pouchdb-adapter-idb": "^6.1.4",
    "@types/pouchdb-adapter-websql": "^6.1.4",
    "@types/pouchdb-core": "^7.0.11",
    "@types/pouchdb-mapreduce": "^6.1.7",
    "@types/pouchdb-replication": "^6.4.4",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.7",
    "@types/react-native": "^0.71.2",
    "@walless/eslint-config": "*",
    "@walless/tsconfig": "*",
    "@wasm-tool/wasm-pack-plugin": "^1.6.0",
    "archiver": "^5.3.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "constructs": "10.2.69",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "fs-extra": "^11.1.0",
    "jest": "^29.4.3",
    "metro-react-native-babel-preset": "0.73.9",
    "mustache": "^4.2.0",
    "react-native": "^0.71.14",
    "react-native-buffer": "^6.0.3",
    "react-native-reanimated": "3.4.2",
    "rimraf": "^5.0.0",
    "shx": "^0.3.4",
    "sst": "2.24.17",
    "superstruct": "^1.0.3",
    "ts-jest": "^29.0.5",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "turbo": "latest",
    "typescript": "^4.9.5",
    "webpack": "^5.79.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@3.6.4"
}
