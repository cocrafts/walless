{
	"extends": "@walless/tsconfig/application.json",
	"include": [
		"index*.js",
		"src"
	],
	"exclude": [
		"node_modules"
	],
	"compilerOptions": {
		"esModuleInterop": true,
		"baseUrl": ".",
		"paths": {
			"components/*": [
				"./src/components/*"
			],
			"stacks/*": [
				"./src/stacks/*"
			],
			"screens/*": [
				"./src/screens/*"
			],
			"utils/*": [
				"./src/utils/*"
			],
			"bridge/*": [
				"./src/bridge/*"
			]
		}
	}
}
